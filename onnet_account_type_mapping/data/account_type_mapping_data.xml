<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="account.type.mapping" id="account_map_invoice">
            <field name="name">Invoice</field>
            <field name="sequence">0</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_receivable')), (4, ref('account.data_account_type_revenue'))]"/>
        </record>
        <record model="account.type.mapping" id="account_map_bill">
            <field name="name">Bill</field>
            <field name="sequence">1</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_payable')), (4, ref('account.data_account_type_current_assets'))]"/>
        </record>
        <record model="account.type.mapping" id="account_map_cost_material">
            <field name="name">Cost, Material</field>
            <field name="sequence">2</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_current_assets')), (4, ref('account.data_account_type_direct_costs')), (4, ref('account.data_account_type_expenses'))]"/>
        </record>
    </data>
</odoo>